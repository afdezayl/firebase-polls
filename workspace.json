{
  "version": 1,
  "projects": {
    "server": {
      "root": "apps/server",
      "sourceRoot": "apps/server/src",
      "projectType": "application",
      "prefix": "server",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/server",
            "main": "apps/server/src/main.ts",
            "tsConfig": "apps/server/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/src/environments/environment.ts",
                  "with": "apps/server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "server:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/server/tsconfig.app.json",
              "apps/server/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/server/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/server/jest.config.js",
            "tsConfig": "apps/server/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "front": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/front",
      "sourceRoot": "apps/front/src",
      "prefix": "polls",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": ["@firebase/app", "firebase/app"],
            "outputPath": "dist/apps/front",
            "index": "apps/front/src/index.html",
            "main": "apps/front/src/main.ts",
            "polyfills": "apps/front/src/polyfills.ts",
            "tsConfig": "apps/front/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/front/src/favicon.ico", "apps/front/src/assets"],
            "styles": ["apps/front/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/front/src/environments/environment.ts",
                  "with": "apps/front/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "front:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "front:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "front:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/front/tsconfig.app.json",
              "apps/front/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/front/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/front/jest.config.js",
            "tsConfig": "apps/front/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/front/src/test-setup.ts"
          }
        }
      }
    },
    "front-e2e": {
      "root": "apps/front-e2e",
      "sourceRoot": "apps/front-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/front-e2e/cypress.json",
            "tsConfig": "apps/front-e2e/tsconfig.e2e.json",
            "devServerTarget": "front:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "front:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/front-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/front-e2e/**/*"]
          }
        }
      }
    },
    "auth-feature-auth-server": {
      "root": "libs/auth/feature-auth-server",
      "sourceRoot": "libs/auth/feature-auth-server/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/auth/feature-auth-server/tsconfig.lib.json",
              "libs/auth/feature-auth-server/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/auth/feature-auth-server/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/auth/feature-auth-server/jest.config.js",
            "tsConfig": "libs/auth/feature-auth-server/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "auth-shared-auth-interfaces": {
      "root": "libs/auth/shared-auth-interfaces",
      "sourceRoot": "libs/auth/shared-auth-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/auth/shared-auth-interfaces/tsconfig.lib.json",
              "libs/auth/shared-auth-interfaces/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/auth/shared-auth-interfaces/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/auth/shared-auth-interfaces/jest.config.js",
            "tsConfig": "libs/auth/shared-auth-interfaces/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-ui-material": {
      "projectType": "library",
      "root": "libs/ui/ui-material",
      "sourceRoot": "libs/ui/ui-material/src",
      "prefix": "polls",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/ui-material/tsconfig.lib.json",
              "libs/ui/ui-material/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ui/ui-material/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/ui-material/jest.config.js",
            "tsConfig": "libs/ui/ui-material/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/ui/ui-material/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "auth-feature-login-web": {
      "projectType": "library",
      "root": "libs/auth/feature-login-web",
      "sourceRoot": "libs/auth/feature-login-web/src",
      "prefix": "polls",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/auth/feature-login-web/tsconfig.lib.json",
              "libs/auth/feature-login-web/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/auth/feature-login-web/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/auth/feature-login-web/jest.config.js",
            "tsConfig": "libs/auth/feature-login-web/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/auth/feature-login-web/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "server"
}
