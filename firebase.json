{
  "functions": {
    "source": "/",
    "predeploy": "npm run build server --prod"
  },
  "hosting": {
    "public": "dist/apps/front",
    "predeploy": "npm run build front -- --prod",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "functions": {
      "host": "0.0.0.0"
    }
  }
}
